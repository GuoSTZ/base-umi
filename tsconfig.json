{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"],
      "guos-components": ["src"],
      "guos-components/*": ["src/*", "*"]
    },
    "resolveJsonModule": true
  },
  "include": [".dumirc.ts", "src/**/*"]
}
